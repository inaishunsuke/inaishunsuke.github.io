Sub NB7_DONE()	'----- Ch番号設定 -----

	Dim	iLocReq0	as Integer
	Dim	iLocReq1	as Integer
	Dim	iLocCh		as Integer
	TB1.TEXT = ""

	IF ((NB7.Value >= 1) AND (NB7.Value <= 3 )) OR ((NB7.Value >= 8) AND (NB7.Value <= 15 )) THEN
		NB6.Active = 3								'設定№
		iLocCh = NB7.Value
		TB1.FG = 14									'Yellow
		TB1.Text = ""								'通信ステータス表示
		iLocReq0 = 0
		iLocReq1 = 0
	ELSE											'Ch範囲異常
		iLocCh = 8
		NB7.Value = iLocCh
		TB1.Text =  "Chは1～3,8～15"
	END IF

End Sub


Sub PB2_CLICKED()	'----- 設定モード移行 -----
	Dim iLineNo as Integer
	TB1.TEXT = ""

	iLineNo = NB7.Value
	IF Not (ConnectCVX(iLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "接続に失敗"
		EXIT SUB
	End IF

	IF NOT (X_S0(iLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "設定ﾓｰﾄﾞ移行失敗"
		EXIT SUB
	END IF
		TB1.FG = -1									'White
		TB1.TEXT = "成功"

	DisConnectCVX iLineNo
	
End Sub

Sub PB3_CLICKED()	'----- 運転モード移行 -----
	Dim iLineNo as Integer
	TB1.TEXT = ""

	iLineNo = NB7.Value
	IF Not (ConnectCVX(iLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "接続に失敗"
		EXIT SUB
	End IF

	IF NOT (X_R0(iLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "運転ﾓｰﾄﾞ移行失敗"
		EXIT SUB
	END IF
		TB1.FG = -1									'White
		TB1.TEXT = "成功"

	DisConnectCVX iLineNo
	
End Sub


Sub NB6_DONE()	'----- 設定№変更 -----
	Dim	iLocSetNo	as Integer
	TB1.TEXT = ""

	IF (NB6.Value < 0) OR (NB6.Value > 999) THEN		'設定№範囲異常
		NB6.Value = 0
		TB1.Text =  "設定は0～999"
	ELSE
		iLocSetNo = NB6.Value						'設定№を整数へ
	END IF

End Sub

Sub NB8_DONE()	'----- SD №変更 -----
	Dim	iSDNo	as Integer
	TB1.TEXT = ""

	IF (NB8.Value < 1) OR (NB8.Value > 2) THEN		'SD №範囲異常
		NB8.Value = 1
		TB1.Text =  "設定は1～2"
	ELSE
		iSDNo = NB8.Value						'SD №を整数へ
	END IF

End Sub

Sub NB9_DONE()	'----- トリガー№変更 -----
	Dim	iTriggerNo as Integer
	TB1.TEXT = ""

	IF ((NB9.Value < -1) OR (NB9.Value > 4)) or (NB9.Value = 0)THEN		'トリガー№範囲異常
		NB8.Value = 1
		TB1.Text =  "設定は-1,1～4"
	ELSE
		iTriggerNo = NB9.Value						'トリガー№を整数へ
	END IF

End Sub


Sub PB4_CLICKED()	'----- 設定№変更実行 -----
	Dim lResult as Long
	Dim iLineNo as Integer
	Dim iProgNo as Integer
	Dim iSDNo as Integer
	TB1.TEXT = ""

	iLineNo = NB7.Value
	IF Not (ConnectCVX(iLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "接続に失敗"
		EXIT SUB
	End IF

	iProgNo = NB6.Value
	iSDNo = NB8.Value
	IF NOT (X_PW(iLineNo, iSDNo, iProgNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "設定No.変更失敗"
		EXIT SUB
	END IF
		TB1.FG = -1									'White
		TB1.TEXT = "成功"

	DisConnectCVX iLineNo
	
End Sub

Sub PB7_CLICKED()	'----- トリガ -----
	Dim iLineNo as Integer
	Dim iTriggerNo as Integer
	Dim strResults as string
	TB1.TEXT = ""
	InitData				'---　受信データクリア

	iLineNo = NB7.Value
	iTriggerNo = NB9.Value
	IF Not (ConnectCVX(iLineNo)) THEN
		TB1.TEXT = "接続に失敗しました"
		EXIT SUB
	End IF

	IF NOT (X_T(iLineNo, iTriggerNo, strResults)) THEN
		TB1.TEXT = "トリガ失敗"
		EXIT SUB
	END IF
		TB1.FG = -1									'White
		TB1.TEXT = "成功"


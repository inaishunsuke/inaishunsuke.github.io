		NB3.Value = D_dnCvxData(12)
		NB4.Value = D_dnCvxData(13)
		NB5.Value = D_dnCvxData(14)
	ELSEIF Label2.Caption = "16" THEN
		NB1.Value = D_dnCvxData(15)
		NB2.Value = D_dnCvxData(16)

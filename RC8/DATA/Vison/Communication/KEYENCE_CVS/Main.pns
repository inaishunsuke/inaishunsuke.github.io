Sub NB7_RELEASED()	'----- Ch番号設定 -----

	Dim	iLocReq0	as Integer
	Dim	iLocReq1	as Integer
	Dim	iLocCh		as Integer
	TB1.TEXT = ""

	IF ((NB7.Value >= 1) AND (NB7.Value <= 3 )) OR ((NB7.Value >= 8) AND (NB7.Value <= 15 )) THEN
		NB6.Active = 3								'設定№
		iLocCh = NB7.Value
		NB7.Value = iLocCh
		TB1.FG = 14									'Yellow
		TB1.Text = ""								'通信ステータス表示
		iLocReq0 = 0
		iLocReq1 = 0
	ELSE											'Ch範囲異常
		iLocCh = 1
		NB7.Value = iLocCh
		TB1.Text =  "Chは1～3,8～15"
	END IF

End Sub


Sub NB6_RELEASED()	'----- 設定№変更 -----
	Dim	iLocSetNo	as Integer
	TB1.TEXT = ""

	IF (NB6.Value < 0) OR (NB6.Value > 999) THEN		'設定№範囲異常
		NB6.Value = 0
		TB1.Text =  "設定は0～999"
	ELSE
		iLocSetNo = NB6.Value						'設定№を整数へ
		NB6.Value = iLocSetNo
	END IF

End Sub


Sub PB2_CLICKED()	'----- 設定№+1 -----
	TB1.TEXT = ""

	IF NB6.Value < 999 THEN							'設定№範囲異常
		NB6.Value = NB6.Value + 1
	END IF

End Sub

Sub PB3_CLICKED()	'----- 設定№-1 -----
	TB1.TEXT = ""

	IF NB6.Value > 0 THEN							'設定№範囲異常
		NB6.Value = NB6.Value - 1
	END IF

End Sub

Sub PB4_CLICKED()	'----- 設定№変更実行 -----
	Dim lResult as Long
	Dim lLineNo as Integer
	Dim lProgNo as Integer
	TB1.TEXT = ""

	lLineNo = NB7.Value
	IF Not (ConnectCV(lLineNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "接続に失敗"
		EXIT SUB
	End IF

	lProgNo = NB6.Value
	IF NOT (X_PW(lLineNo, lProgNo)) THEN
		TB1.FG = 14									'Yellow
		TB1.TEXT = "設定No.変更失敗"
		EXIT SUB
	END IF
		TB1.FG = -1									'White
		TB1.TEXT = "成功"

	DisconnectCV lLineNo
	
End Sub

Sub PB7_CLICKED()	'----- トリガ -----

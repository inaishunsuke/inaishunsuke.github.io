	Dim lLineNo as Integer
	Dim lProgNo as Integer
	Dim	iLocDataPage	as Integer
	Dim strResults as string
	TB1.TEXT = ""
	InitData				'---　受信データクリア

	lLineNo = NB7.Value
	IF Not (ConnectCV(lLineNo)) THEN
		TB1.TEXT = "接続に失敗しました"
		EXIT SUB
	End IF

	IF NOT (X_T1(lLineNo, strResults)) THEN
		TB1.TEXT = "トリガ失敗"
